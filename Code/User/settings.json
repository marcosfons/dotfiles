{
  "[dart]": {
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "window.titleBarStyle": "custom",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "**/.fvm/flutter_sdk/**": true,
    "**/build/**": true,
    "**/.dart_tool/**": true,
    "**/.gradle/**": true,
    "**/.idea/**": true,
    "**/linux/flutter/**": true,
    "**/windows/flutter/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.fvm/**": true,
    "**/build/**": true,
    "**/.dart_tool/**": true,
    "**/.gradle/**": true,
    "**/linux/flutter/**": true,
    "**/windows/flutter/**": true
  },
  "explorer.confirmDragAndDrop": false,
  "terminal.external.linuxExec": "/bin/bash",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.dart_tool/": true,
    "**/.idea/": true,
    "**/.flutter-plugins*": true,
    "**/minhaprefeitura.iml": true
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorCustomizations": {
    "editor.background": "#0d1117",
    "sideBar.background": "#0d1117",
    "panel.background": "#0d1117",
    "activityBar.background": "#0a0c0e",
    "menu.background": "#0d1117"
  },
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "'Dejavu Sans Mono', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
  "python.insidersChannel": "weekly",
  "jupyter.askForKernelRestart": false,
  "dart.renameFilesWithClasses": "prompt",
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.maxColumn": 60,
  "editor.quickSuggestionsDelay": 50,
  "telemetry.telemetryLevel": "off",
  "explorer.compactFolders": false,
  "breadcrumbs.enabled": false,
  "vetur.validation.template": false,
  "vetur.validation.script": false,
  "browse-lite.chromeExecutable": "/usr/bin/chromium",
  "vite.autoStart": false,
  "redhat.telemetry.enabled": false,
  "diffEditor.renderSideBySide": false,
  "cmake.configureOnOpen": false,
  "jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
  ],
  "notebook.output.textLineLimit": 50,
  "editor.mouseWheelScrollSensitivity": 1.3,
  "workbench.list.mouseWheelScrollSensitivity": 1.3,
  "editor.lineNumbers": "relative",
  
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "mssql.connections": [
    {
      "server": "192.168.2.24,1433",
      "database": "",
      "authenticationType": "SqlLogin",
      "user": "admin",
      "password": "",
      "emptyPasswordInput": false,
      "savePassword": true,
      "profileName": "IBOR",
      "encrypt": "Mandatory",
      "trustServerCertificate": true,
      "connectTimeout": 15,
      "commandTimeout": 30,
      "applicationName": "vscode-mssql"
    }
  ],
  "python.analysis.extraPaths": [
    "./.venv"
  ],
  "terminal.integrated.tabs.enabled": false,
  "[handlebars]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right"
  },
  "notebook.lineNumbers": "on",
  "workbench.colorTheme": "GitHub Dark",
  "latex-workshop.latex.outDir": "build",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-e>": false
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "editor.stickyScroll.maxLineCount": 3,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "editor.stickyScroll.enabled": true
}
